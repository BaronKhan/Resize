    else
    {
        draw_text_transformed(display_get_gui_width()/2 ,(display_get_gui_height()/2)-(40*scale),"Time: "+string(minutes)+".0"+string(seconds)+" s",scale,scale,0)    
    }

}
else
{
    time = time_to_3
    draw_set_valign(fa_middle)

    //draw_set_color(c_black)
    draw_set_halign(fa_center)

    minutes = floor(time/room_speed)
    seconds = floor(((time % room_speed)/60)*99)

    draw_set_font(font_missinglink16)
    if seconds &gt;= 10
    {
        draw_text_transformed(display_get_gui_width()/2 ,(display_get_gui_height()/2)-(40*scale),"Time needed for#3 Stars: "+string(minutes)+"."+string(seconds)+" s",scale,scale,0)
    }
    else
    {
        draw_text_transformed(display_get_gui_width()/2 ,(display_get_gui_height()/2)-(40*scale),"Time needed for#3 Stars: "+string(minutes)+".0"+string(seconds)+" s",scale,scale,0)    
    }
    draw_set_valign(-1)
}


}
else
{
    //boss
    draw_set_halign(fa_center)


    draw_set_font(font_missinglink16)
    draw_text_transformed(display_get_gui_width()/2 ,(display_get_gui_height()/2)-(40*scale),"Well Done!",scale,scale,0)    
    
}

draw_set_font(-1)

draw_set_color(c_white)
draw_set_halign(fa_left)
